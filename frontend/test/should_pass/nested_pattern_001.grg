test t {
	(.-->.<--.;)*
	(.-->.<--.;)+
	(.-->.<--.;)?
	(.-->.<--.;|.<--.-->.;)
	~(.<--.-->.; ~(n:Node-->.-->n;))
	&(.-->.<--.; &(n:Node-->.-->n;))
}

rule r {
	(.-->n:Node<--.; {+ n--n;})*
	(.-->n:Node<--.; {+ n--n;})+
	(.-->n:Node<--.; {+ n--n;})?
	(.-->n:Node<--.; {+ n--n;})[3]
	(.-->n:Node<--.; {+ n--n;})[2:3]
	(.-->n:Node<--.; {+ n--n;})[42:*]
	(.-->n:Node<--.; {- n--n;} | .<--n:Node-->.; {- n-->n;})
	~(.<--n:Node-->.; ~(m:Node-->.-->m;))
	&(.-->n:Node<--.; &(m:Node-->.-->m;))

	{+
		n:Node-->.-->n;
	}
}
