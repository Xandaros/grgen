function foo() : double
{
	def var temp1:double = sin(0.0);
	def var temp2:double = cos(0.0);
	def var temp3:double = tan(0.0);
	return(temp1+temp2+temp3);
}

test t : (double)
{
	def var res:double;
	
	independent {
		yield {
			def var temp1:double = sin(0.0);
			def var temp2:double = cos(0.0);
			def var temp3:double = tan(0.0);
			yield res = temp1+temp2+temp3;
		}
	}
	
	return (res);
}

rule r : (double)
{	
	if(sin(0.0)+cos(0.0) == tan(0.0));
	
	modify {
		def var res:double;
		
		eval { 
			def var temp1:double = sin(0.0);
			def var temp2:double = cos(0.0);
			def var temp3:double = tan(0.0);
			yield res = temp1+temp2+temp3;
		}
		
		return (res);
	}
}
