node class N
{
	name:string;
	counter:int;
	
	function foo(var x:int) : int
	{
		return (this.counter + x);
	}
	
	procedure bar(var x:int, var y:int) : (int, int)
	{
		return (this.counter + x, this.counter - y);
	}
	
	procedure baz()
	{
		this.counter = this.counter + 1;
		this.name = this.name + "baz";
		return;
	}
	
	function qux() : string
	{
		return(this.name);
	}
}

