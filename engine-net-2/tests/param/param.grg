rule init2 : (Edge, Edge, Edge)
{
    a:Node;
    b:Node;
    c:Node;
    d:Node;

    modify {
        a -e:Edge-> b;
        a --> b;
        b -f:Edge-> c;
        c <-- d;
        b --> d;
        d --> a;
        d --> a;
        d -r:Edge-> d;
        
        return(e,f,r);
    }
}

