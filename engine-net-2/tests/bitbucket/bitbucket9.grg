// test based on bitbucket issue #9 by Peter Grüner

sequence TestSequence {
    {add(Node); emit("Created one node!\n");} &
    [DeleteRule] &
    [DeleteRule]
}

rule DeleteRule {
    n:Node;
    modify{
        emithere("1 value of attribute found node " + nameof(n) +"\n");
        delete(n);
    }
}
