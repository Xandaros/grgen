new graph random/random1

randomseed 0 # get deterministic random for tests

xgrs ::one=1
xgrs ::two=2
xgrs ::three=3

xgrs $[matcht]
xgrs $::three[matcht]
xgrs $::two,::three[matcht]
xgrs $::three,::three[matcht]

new n1:A

xgrs $[matcht]
xgrs $::three[matcht]
xgrs $::two,::three[matcht]
xgrs $::three,::three[matcht]

new n2:A

xgrs $[matcht]
xgrs $::three[matcht]
xgrs $::two,::three[matcht]
xgrs $::three,::three[matcht]

new n3:A

show num nodes only A
show num nodes only B

xgrs $::two,::two[changetype]

show num nodes only A
show num nodes only B

xgrs $::one,*[changetype]

show num nodes only A
show num nodes only B

exit
