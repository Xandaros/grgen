#using "index_model_smoke.gm"

rule ri {
	n:N{ascending(fooi>1, fooi<3)} -e:E{bari==n.i}-> n;
	
	modify {
		emit("The value of attribute i is " + n.i + "\n");
		emit("The value of attribute i is " + e.i + "\n");
	}
}

