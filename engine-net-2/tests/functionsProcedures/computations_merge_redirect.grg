using model_init;

pinit : (A,Edge,Node)
{
	(def n1:A) = add(A);
	(def n2:Node) = add(Node);
	(def -e:Edge->) = add(Edge,n1,n2);
	return(n1,e,n2);
}

pclear
{
	clear();
	return;
}

pmerge(n1:A, -e:Edge->, n2:Node)
{	
	merge(n1,n2);
	return;
}

test tmerge
{
	n1n2:A --> n1n2;
}

predirectsource(n1:A, -e:Edge->, n2:Node)
{	
	redirectSource(e,n2);
	return;
}

test tredirectsource
{
	n1:A;
	n2:Node --> n2;
}

predirecttarget(n1:A, -e:Edge->, n2:Node)
{	
	redirectTarget(e,n1);
	return;
}

test tredirecttarget
{
	n1:A --> n1;
	n2:Node;
}

predirctsourcetarget(n1:A, -e:Edge->, n2:Node)
{	
	redirectSourceAndTarget(e,n2,n1);
	return;
}

test tredirectsourcetarget
{
	n1:A <-- n2:Node;
}
