rule matchmark : (Node)
{
	n:Node;
	if { !n.visited; }
	
	modify {
		eval { n.visited = true; }
		return (n);
	}
}

rule setstring(n:N)
{	
	modify {
		eval { n.s = "S"; }
	}
}

rule unsetstring(n:N)
{	
	modify {
		eval { n.s = ""; }
	}
}
