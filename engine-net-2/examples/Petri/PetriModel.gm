node class PetriNet;

node class Place;

node class Transition;

node class Token;

edge class places
	connect PetriNet[*] --> Place[0:1];

edge class transitions
	connect PetriNet[*] --> Transition[0:1];

edge class tokens
	connect Place [*] --> Token[1];

edge class inArc
	connect Transition [*] --> Place [*];

edge class outArc
	connect Place [*] --> Transition [*];

edge class inhibitorArc
	connect Place [*] --> Transition [*];
