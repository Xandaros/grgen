new graph "Snowflake.grg"

new a:Node1
new b:Node1
new c:Node1
new a-:Edge1->b
new b-:Edge1->c
new c-:Edge1->a

debug set layout Organic
dump set edge Edge1 color red
dump set edge Edge2 color blue
dump set edge EdgeSperr color grey

debug xgrs (makeFlake1* & beautify* & makeFlake2* & beautify*)[1]

